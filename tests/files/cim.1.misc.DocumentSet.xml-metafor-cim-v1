                  <shortName>Method</shortName>
                  <longName>Method</longName>
                  <value>use same turbulence scheme as AtmosphericChemistry</value>
                </componentProperty>
                <!--Constraint start: if Method is "specific turbulence scheme"-->
                <!--Constraint is invalid-->
                <!--Constraint end: if Method is "specific turbulence scheme"-->
              </componentProperty>
            </componentProperties>
